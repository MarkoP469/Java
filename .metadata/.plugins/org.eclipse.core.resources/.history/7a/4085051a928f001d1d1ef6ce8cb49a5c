package edunova01;

import javax.swing.JOptionPane;

public class varijabla {

	public static void main(String[] args) {
		
		
		int i = Integer.parseInt(
				JOptionPane.showInputDialog("prviBroj"));
		int j = Integer.parseInt(
				JOptionPane.showInputDialog("drguiBroj"));
		for (i=1; i<j;i++) {
			if (i/1==i && i/i==1)
			{
				System.out.println(i+j);
			}
		}
		}
	}
