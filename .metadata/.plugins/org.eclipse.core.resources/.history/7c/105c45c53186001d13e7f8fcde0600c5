package edunova01;

import javax.swing.JOptionPane;

public class varijabla {

	public static void main(String[] args) {
		
		int i = Integer.parseInt(
				JOptionPane.showInputDialog("unesi broj od 1 do 999"));
		
		if(i>0 && i<1000) {
			System.out.println("ovo radi");
			} 
		else {
			System.out.println(i + "greska");
			}
	}

}
