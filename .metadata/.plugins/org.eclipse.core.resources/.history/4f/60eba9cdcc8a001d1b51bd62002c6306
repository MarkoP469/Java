package edunova01;

import javax.swing.JOptionPane;

public class varijabla {

	public static void main(String[] args) {
		
		int broj;
		for(;;) {
			broj = Integer.parseInt(
					JOptionPane.showInputDialog(
							"Unesi broj izmeÄ‘u 1 i 10")
					);
			if(broj>=1 && broj<=10) {
				break;
			}
		}
		System.out.println(broj);
	
	}

}
