package edunova01;

import javax.swing.JOptionPane;

public class varijabla {

	public static void main(String[] args) {
		
		int niz[] = new int[3S];
		int zbroj=0;
		for(int i=0;i<niz.length;i++) {
			niz[i] = Integer.parseInt(
					JOptionPane.showInputDialog(
							"Unesi " + (i+1) + ". broj")
					);
			
			 zbroj+=niz[i];
			 System.out.println(zbroj);
			}
		
	}

}
